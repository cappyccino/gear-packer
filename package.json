  "main": "js/index.js",
  "files": ["js/"],
  "config": {
    "port": "8080"
  },
  "scripts": {
    "test": "jest",
    "start": "webpack serve --config ./js/webpack.config.js --mode development",
    "build": "webpack --mode=production"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": ["<rootDir>/integration/"],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/js/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/js/__mocks__/fileMock.js",
      "\\.(css|sass|scss)$": "<rootDir>/js/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "jest": "^27.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
